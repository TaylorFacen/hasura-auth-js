    "jest": "^26.6.3",
    "jest-extended": "^0.11.5",
    "ts-jest": "^26.1.0",
    "typescript": "^4.4.2"
  },
  "jest": {
    "verbose": true,
    "testEnvironment": "jsdom",
    "globalSetup": "./src/test/globalSetup.ts",
    "globalTeardown": "./src/test/globalTeardown.ts",
